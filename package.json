{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@monorepo-nx-vite-vue3-vuetify-ts/source",
  "description": "",
  "version": "0.0.0",
  "license": "GNU",
  "repository": {
    "url": "https://github.com/saan800/monorepo-nx-vite-vue3-vuetify-ts"
  },
  "keywords": [
    "nx",
    "vite",
    "vue",
    "vue3",
    "vuetify",
    "pinia",
    "typescript"
  ],
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "nx serve my-app --configuration=development",
    "preview": "nx preview my-app --configuration=development",
    "build": "nx build my-app --configuration=production",
    "build:stats": "nx build my-app --configuration=production --skipNxCache",
    "lint": "nx run-many -t lint --cached --silent",
    "lint:fix": "nx run-many -t lint --fix --cached --silent",
    "lint:spellcheck": "cspell --no-progress --show-suggestions --color \"**\"",
    "test:ci": "nx run-many -t test --coverage --reporters junit",
    "test:unit": "nx run-many -t test",
    "test:watch": "nx run-many -t test --watch",
    "test:e2e": "nx run-many -t e2e"
  },
  "dependencies": {
    "@mdi/js": "^7.3.67",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vuetify": "^3.3.21",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@nx/cypress": "^16.10.0",
    "@nx/eslint-plugin": "^16.10.0",
    "@nx/js": "^16.10.0",
    "@nx/linter": "^16.10.0",
    "@nx/vite": "^16.10.0",
    "@nx/workspace": "^16.10.0",
    "@nxext/vue": "^16.7.0",
    "@types/node": "^20.8.6",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "browserslist-to-esbuild": "^1.2.0",
    "cspell": "^7.3.8",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuetify": "^2.0.5",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "nx": "^16.10.0",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.3",
    "typescript": "^5.1.3",
    "vite": "^4.4.11",
    "vite-plugin-vuetify": "^1.0.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  }
}
